perm	USE_A,1
perm	USE_B,1
func	SecurityAdd
var		int,a(0)
var		int,b(0)
trans	r0,x0
store	a(0),r0
trans	r0,x1
store	b(0),r0
load	r0,a(0)
load	r1,b(0)
add		r0,r1
trans	y,r0
ret		
func	main
load	r0,1
load	r1,1
trans	x0,r0
trans	x1,r1
call	SecurityAdd
trans	r0,y
test	r1,<USE_B,USE_A>
jmpf	0,r1
output	r0
lable	0
output	endline
